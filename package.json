{
  "name": "react-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "webpack-dev-server --env=dev --progress --hot --profile --colors",
    "start:dist": "webpack-dev-server --env=prod --progress --hot --profile --colors",
    "build:dev": "rm -rf dist && webpack --env=dev --progress",
    "build:dist": "rm -rf dist && webpack --env=prod --progress --profile --colors",
    "deploy": "npm run build:dist && npm run surge",
    "surge": "surge -p dist --domain https://mywords.surge.sh",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-hot-boilerplate",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.0",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.22.0",
    "babili-webpack-plugin": "0.0.10",
    "copy-webpack-plugin": "=2.1.6",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-config-mycs": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "install": "^0.8.7",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.0",
    "react-hot-loader": "^1.3.1",
    "sass-loader": "^5.0.1",
    "serviceworker-webpack-plugin": "^0.1.8",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.16.1",
    "write-file-webpack-plugin": "^3.4.2"
  },
  "dependencies": {
    "antd": "^2.7.1",
    "babel-runtime": "^6.23.0",
    "is-online": "^6.1.0",
    "lodash.uniqby": "^4.7.0",
